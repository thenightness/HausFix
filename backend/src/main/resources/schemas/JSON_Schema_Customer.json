{
  "title": "Customer-JSON-Schema",
  "type": "object",
  "required": [
    "customer"
  ],
  "properties": {
    "customer": {
      "type": "object",
      "required": [
        "firstName",
        "lastName",
        "gender"
      ],
      "properties": {
        "uuid": {
          "type": [
            "string",
            "null"
          ]
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "birthDate": {
          "type": "string",
          "format": "date"
        },
        "gender": {
          "type": "string",
          "enum": [
            "D",
            "M",
            "U",
            "W"
          ]
        }
      }
    }
  }
}